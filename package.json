{
  "name": "unipept-desktop",
  "version": "2.1.0",
  "productName": "Unipept Desktop",
  "private": false,
  "author": "Unipept Team (Ghent University)",
  "description": "A desktop equivalent of unipept.ugent.be. This app aims at high-throughput analysis of metaproteomics samples.",
  "repository": {
    "type": "git",
    "url": "https://github.com/unipept/unipept-desktop.git"
  },
  "main": "./out/main/index.js",
  "homepage": "https://unipept.ugent.be",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --publish always --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@electron/remote": "^2.0.10",
    "@types/async": "^3.2.21",
    "@types/better-sqlite3": "^7.6.5",
    "@types/uuid": "^9.0.4",
    "async": "^3.2.4",
    "better-sqlite3": "^9.4.0",
    "compare-versions": "^6.1.0",
    "electron-updater": "^6.1.4",
    "follow-redirects": "^1.15.5",
    "marked": "^7.0.1",
    "pinia": "^2.1.6",
    "unipept-web-components": "^2.1.5",
    "uuid": "^9.0.1",
    "vue-router": "4",
    "vuetify": "^3.5.3"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@mdi/font": "^7.2.96",
    "@rushstack/eslint-patch": "^1.3.0",
    "@types/marked": "^5.0.1",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "electron": "^28.2.2",
    "electron-builder": "^24.6.4",
    "electron-vite": "^1.0.28",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "less": "^4.1.3",
    "sass": "^1.64.2",
    "typescript": "^5.1.3",
    "vite": "^5.0.12",
    "vue": "^3.4.16",
    "vue-tsc": "^1.6.5"
  }
}
