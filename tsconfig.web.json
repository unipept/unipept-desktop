{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.vue",
    "src/common/**/*",
    "src/preload/*.d.ts"  
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "./src/renderer/*"
      ],
      "@common/*": [
        "./src/common/*"
      ]
    }
  }
}
